﻿@{
	Layout = "";
	if (Convert.ToInt32(Session["Level"]) == 1)
	{
		Layout = "/Views/Layout/SuperAdminLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 2)
	{
		Layout = "/Views/Layout/FinanceLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 3)
	{
		Layout = "/Views/Layout/CashierLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 4)
	{
		Layout = "/Views/Layout/ReservationLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 5)
	{
		Layout = "/Views/Layout/RegistrationLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 6)
	{
		Layout = "/Views/Layout/OICLayout.cshtml";
	}
	else if (Convert.ToInt32(Session["Level"]) == 7)
	{
		Layout = "/Views/Layout/SecurityGuardLayout.cshtml";
	}

}









﻿<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>



	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")

</body>
</html>
