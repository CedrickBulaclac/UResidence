<script>
     $(document).ready(
        function () {
            var visit = @Convert.ToInt32(Session["aa"]);
            if ('@ViewBag.s' == 'True') {
                if (visit == 0) {
                    $('#myModal').modal();
                }

			}

			var a = '@ViewBag.successModal';
			if (a == 'True') {
				$('#myModalSucess').modal();
				a = false;
             }
		});

	$("#btnAbout").click(function () {
		$('html,body').animate({
			scrollTop: $(".second").offset().top
		},
			'slow');
	});

	$("#btnContact").click(function () {
		$('html,body').animate({
			scrollTop: $(".third").offset().top
		},
			'slow');
	});
</script>
