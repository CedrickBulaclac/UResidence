
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Reserve/Layout.cshtml";
}
<footer>
    <div id="aaerror" class="alert alert-danger collapse">
        <strong>Please settle your account before you can reserve.</strong>
    </div>
</footer>
<script type="text/javascript">

    $(document).ready(
        function () {
      
            if (@ViewBag.Status == true) {
                $('#aaerror').show('fade');
                setTimeout(function () {
                    $('#aaerror').hide('fade');
                }, 2000);
            }

        }
    );




</script>