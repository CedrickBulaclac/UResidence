<div class="container">
	<div class="modal fade" id="myModal" role="dialog">
		<div class="modal-dialog" style="position:initial;">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>

				</div>
				<div class="modal-body">
					<strong>Please settle your account before you can reserve.</strong>
				</div>

				<div class="modal-footer">
					<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
				</div>
			</div>
		</div>
	</div>
</div>


<!-- Modal HTML -->
<div id="myModalSucess" class="modal fade" style="height:600px">
	<div class="modal-dialog modal-confirm" style="position:initial;">
		<div class="modal-content">
			<div class="modal-header">
				<div class="icon-box">
					<i class="material-icons">&#xE876;</i>
				</div>
				<h4 class="modal-title">Reservation successfully submitted.</h4>
			</div>
			<div class="modal-body">
				<p class="text-center">
					NOTE: This is just pencil booking reservation,
					Kindly settle your payment within 48 hours to confirm your reservation.
				</p>
			</div>
			<div class="modal-footer">
				<button class="btn btn-success btn-block" data-dismiss="modal">OK</button>
			</div>
		</div>
	</div>
</div>

<br />

