    <script>
        $(document).ready(function () {
            var table = $('#TenantData').DataTable({
                "ajax": {
                    "url": '/Tenant/GetTenant',
                    "type": "GET",
                    "datatype": "json",
                },
                "columns": [
                    { "data": "BldgNo", "autowidth": true },
                    { "data": "UnitNo", "autowidth": true },
                    { "data": "Fullname", "autowidth": true },
                    { "data": "Bdate", render: function (data, type, announcement) { return announcement.FormattedDate }, "autowidth": true },
                    { "data": "CelNo", "autowidth": true },
                    { "data": "Email", "autowidth": true },
                    { "data": "Lease", "autowidth": true },
